{
	"name": "markdownlinkcompletionprovider",
	"displayName": "Markdown Link Completion Provider",
	"description": "Provides completion for markdown links",
	"version": "0.1.0",
	"engines": {
		"vscode": "^1.59.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onStartupFinished"
	],
	"main": "./out/extension.js",
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"watch": "tsc -watch -p ./",
		"pretest": "npm run compile && npm run lint",
		"lint": "eslint src --ext ts",
		"test": "node ./out/test/runTest.js"
	},
	"devDependencies": {
		"@types/vscode": "^1.59.0",
		"@types/glob": "^7.1.3",
		"@types/mocha": "^8.2.2",
		"@types/node": "14.x",
		"eslint": "^7.27.0",
		"@typescript-eslint/eslint-plugin": "^4.26.0",
		"@typescript-eslint/parser": "^4.26.0",
		"glob": "^7.1.7",
		"mocha": "^8.4.0",
		"typescript": "^4.3.2",
		"vscode-test": "^1.5.2"
	},
	"publisher": "Klaus Jandl",
    "repository": {
        "type": "git",
        "url": "https://github.com/klausj1/MarkdownLinkCompletionProvider.git"
      },
      "homepage": "https://github.com/klausj1/MarkdownLinkCompletionProvider/blob/main/README.md"
}
